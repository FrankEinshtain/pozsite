{
  "name": "pozitiff-site",
  "private": true,
  "description": "Pozitiff Site New",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "styleLint": "stylelint .",
    "styleLint-fix": "stylelint . --fix",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-gatsby-standard": "^2.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "stylelint": "^13.6.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.17.2"
  },
  "dependencies": {
    "gatsby": "^2.22.15",
    "gatsby-image": "^2.4.7",
    "gatsby-plugin-netlify-cms": "^4.3.5",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.3.4",
    "gatsby-plugin-sass": "^2.3.4",
    "gatsby-plugin-sharp": "^2.6.12",
    "gatsby-source-filesystem": "^2.3.11",
    "gatsby-transformer-remark": "^2.8.17",
    "gatsby-transformer-sharp": "^2.5.5",
    "netlify-cms-app": "^2.12.15",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-slick": "^0.26.1",
    "slick-carousel": "^1.8.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "pretty-quick",
      "eslint --fix",
      "stylelint . --fix",
      "git add"
    ],
    "*.{json,html}": [
      "pretty-quick",
      "git add"
    ],
    "*.{css,scss}": [
      "stylelint . --fix",
      "pretty-quick",
      "git add"
    ]
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
